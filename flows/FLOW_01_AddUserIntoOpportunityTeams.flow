    <decisions>
        <name>Checking_User</name>
        <label>Checking User</label>
        <locationX>1379</locationX>
        <locationY>131</locationY>
        <defaultConnector>
            <targetReference>looping_Metadata_Records</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Map_User_To_Team</name>
            <conditionLogic>(1 AND 2 AND 3 AND 4 AND 10 AND 11) OR (1 AND 2 AND 12 AND 4 AND 10 AND 11) OR(5 AND 2 AND 3 AND 4 ) OR (6 AND 2 AND 3 AND 4 )  OR (7 AND 2 AND 3 AND 4 )  OR (7 AND 2 AND 12 AND 4 )  OR (8 AND 2 AND 3 AND 4 )  OR (9 AND 2 AND 3 AND 4 ) OR (9 AND 2 AND 4 AND 12)</conditionLogic>
            <conditions>
                <leftValueReference>looping_Metadata_Records.MasterLabel</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$Label.Yat_Kan_Yu_UserLabel</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>looping_Metadata_Records.BU_Application1__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$Record.BU_Application_1__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>looping_Metadata_Records.Bu_Application2__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$Record.BU_Application_2__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>looping_Metadata_Records.Region__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$Record.Region__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>looping_Metadata_Records.MasterLabel</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$Label.Guillaume_Jacque_UserLabel</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>looping_Metadata_Records.MasterLabel</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$Label.Bruno_Guillemat_Userlabel</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>looping_Metadata_Records.MasterLabel</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$Label.Fanny_Deplace_Userlabel</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>looping_Metadata_Records.MasterLabel</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$Label.Oc_ane_Lamarzelle_UserLabel</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>looping_Metadata_Records.MasterLabel</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$Label.Lolita_Hauguel_UserLabel</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Account.BillingCountry</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>India</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Account.BillingCountry</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Bangladesh</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.BU_Application_2__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Opportunity_Team</targetReference>
            </connector>
            <label>Map User To Team</label>
        </rules>
