        <formula>IF ( INCLUDES ( Opportunity.Active_BUs__c  , &quot;Functional Polyolefins&quot; ), &quot;SKFP; &quot;,null ) &amp;
IF ( INCLUDES (Opportunity.Active_BUs__c  , &quot;SKGC&quot; ), &quot;SKGC; &quot;,null ) &amp;
